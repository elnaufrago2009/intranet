<div class="page-header">
  <h1>Tipos de Documentos</h1>
</div>


<table class="table table-striped table-bordered" >
  <tr>
    <th>#Id</th>
    <th>Nombre</th>
    <th>Acciones</th>
  </tr>

<% @tipodocumentos.each do |tipodocumento| %>
  <tr>
    <td><%= tipodocumento.id %></td>
    <td><%= tipodocumento.nombre %></td>
    <td>
      <%= link_to 'Ver', tipodocumento, :class => "btn" %>
      <%= link_to 'Editar', edit_tipodocumento_path(tipodocumento), :class => "btn" %>
      <%= link_to 'Borrar', tipodocumento, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn" %>
    </td>
  </tr>
<% end %>
</table>

<div class="form-actions">
  <%= link_to 'Nuevo Tipo Documento', new_tipodocumento_path, :class => "btn btn-primary" %>
</div>


